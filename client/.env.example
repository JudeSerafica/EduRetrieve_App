# Frontend Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration
# Get from: https://supabase.com/dashboard
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your_anon_key_here

# API Base URL - CRITICAL FOR PRODUCTION
# This is the URL of your deployed Express backend
# - Leave empty for localhost development (works with proxy)
# - Set to your deployed backend URL for production
# Examples:
#   REACT_APP_API_BASE_URL=https://your-backend.railway.app
#   REACT_APP_API_BASE_URL=https://your-backend.fly.dev
#   REACT_APP_API_BASE_URL=https://your-backend.onrender.com
REACT_APP_API_BASE_URL=

# Google OAuth Client ID (optional - for Google sign-in)
# Get from: https://console.cloud.google.com/
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here
